[library]
version = "@version@"
authors = "Treecase"
license = "GPL-3.0-or-later"
website_url = "https://treecase.github.io/rmf-glib"
browse_url = "https://github.com/treecase/rmf-glib"
description = "RMF loading library"
devhelp = true
search_index = true
dependencies = ["GLib-2.0", "GObject-2.0", "Gio-2.0"]

  [dependencies."GLib-2.0"]
  name = "GLib"
  description = "The base utility library"
  docs_url = "https://docs.gtk.org/glib/"

  [dependencies."GObject-2.0"]
  name = "GObject"
  description = "The base type system library"
  docs_url = "https://docs.gtk.org/gobject/"

  [dependencies."Gio-2.0"]
  name = "Gio"
  description = "GObject Interfaces and Objects, Networking, IPC, and I/O"
  docs_url = "https://docs.gtk.org/gio/"

[theme]
name = "basic"
show_index_summary = true
show_class_heirarchy = true

[source-location]
base_url = "https://github.com/treecase/rmf-glib/blob/main"

[extra]
content_files = ['about-rmf.md']
content_images = []
urlmap_file = "urlmap.js"

